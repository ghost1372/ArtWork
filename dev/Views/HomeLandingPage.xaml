<?xml version="1.0" encoding="utf-8" ?>
<Page x:Class="ArtWork.Views.HomeLandingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:dev="using:DevWinUI"
      xmlns:local="using:ArtWork"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d">
    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Light">
                    <x:String x:Key="HeaderCover">/Assets/Cover/CoverLight.png</x:String>
                </ResourceDictionary>
                <ResourceDictionary x:Key="Dark">
                    <x:String x:Key="HeaderCover">/Assets/Cover/CoverDark.png</x:String>
                </ResourceDictionary>
            </ResourceDictionary.ThemeDictionaries>
        </ResourceDictionary>
    </Page.Resources>
    <dev:AllLandingPage HeaderImage="{ThemeResource HeaderCover}"
                        HeaderOverlayImage="{ThemeResource HeaderCover}"
                        HeaderSubtitleText="Thosands Art"
                        HeaderText="{x:Bind dev:ProcessInfoHelper.ProductNameAndVersion}"
                        JsonNavigationService="{x:Bind local:App.Current.GetJsonNavigationService}" />

</Page>

